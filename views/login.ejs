<%- include ("partials/signupLoginHeader") -%>

    
    <main class="form-signin w-100 m-auto">
        <form name="loginForm" action="/login" method="post">
          <h1 class="h3 mb-3 fw-normal">Login</h1>
       
      
          
       
          <div class="form-floating">
              <input type="email" name="email" id="email" class="form-control middle"  placeholder="name@example.com" required>
              <label for="email">Email address</label>
            </div>
            <div class="form-floating">
              <input type="password" name="password" id="password" placeholder="password" class="form-control bottom" required>
              <label for="password">Password</label>
            </div>
            
      
          
          <button class="w-100 btn btn-lg btn-primary" type="submit">Sign in</button>
          <p class="mt-5 mb-3 text-muted">&copy; Champs 4 Grabs 2022</p>
        </form>
      </main>
      
      <%- include ("partials/signupLoginFooter") -%>

      
      
